<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://schemas.getenvy.com/BookAll.xsd" xmlns="http://www.w3.org/2001/XMLSchema">

	<element name="bookAlls">
		<complexType>
			<element name="bookAll" type="allType" minOccurs="1" maxOccurs="unbounded"/>
		</complexType>
	</element>
	<complexType name="allType">
		<all>
			<attribute name="title" type="string"/>
		</all>
	</complexType>

</schema>