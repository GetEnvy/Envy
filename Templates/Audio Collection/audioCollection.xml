<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://schemas.getenvy.com/Template.xsd" version="1.0">
<!-- Originally created by Tom Higginson -->
<manifest	name="Audio Collection"
			author="Envy"
			version="1.0"
			link="http://getenvy.com/"
			description="Audio collection export template."
			icon="audio.ico"
/>
<files>
	<file type="main" path="audio.tpl"/>
	<file type="evenfile" path="audioEven.tpl"/>
	<file type="oddfile" path="audioOdd.tpl"/>
</files>
<images>
	<image path="bar_on.gif"/>
	<image path="bar_off.gif"/>
</images>
<wizardText>
	<text language="pt">
		<item id="1" type="textbox" value="Título da coleção"/>
		<item id="2" type="textbox" value="Ao abrir arquivo" default="Abrir Arquivo"/>
		<item id="3" type="textbox" value="Enquanto baixando" default="Baixando"/>
		<item id="4" type="textbox" value="Ação de download" default="Baixar Agora"/>
		<item id="6" type="textbox" value="Descrição de Recurso da Rede"/>
		<item id="7" type="colorpicker" value="Colorir fileiras ímpares" default="#FFFFFF"/>
		<item id="8" type="colorpicker" value="Colorir fileiras pares" default="#E9EDEF"/>
	</text>
	<text language="es">
		<item id="1" type="textbox" value="Título de la colección"/>
		<item id="2" type="textbox" value="Acción de apertura del archivo" default="Abrir archivo"/>
		<item id="3" type="textbox" value="Mientras se descarga" default="Descarga"/>
		<item id="4" type="textbox" value="Descarga acción" default="Descargar ahora"/>
		<item id="6" type="textbox" value="Descripción de los recursos Web "/>
		<item id="7" type="colorpicker" value="Color para filas incluso" default="#FFFFFF"/>
		<item id="8" type="colorpicker" value="Color para filas impares" default="#E9EDEF"/>
	</text>
	<text language="fr">
		<item id="1" type="textbox" value="Titre de collection"/>
		<item id="2" type="textbox" value="Action d'ouverture de fichier" default="Ouvrir un fichier"/>
		<item id="3" type="textbox" value="Lors du téléchargement" default="Téléchargement"/>
		<item id="4" type="textbox" value="Télécharger l'action" default="Télécharger"/>
		<item id="6" type="textbox" value="Description des ressources Web"/>
		<item id="7" type="colorpicker" value="Couleur des lignes même" default="#FFFFFF"/>
		<item id="8" type="colorpicker" value="Couleur des lignes impaires" default="#E9EDEF"/>
	</text>
	<text language="de">
		<item id="1" type="textbox" value="Sammlung Titel"/>
		<item id="2" type="textbox" value="Dateiöffnungsvorgang" default="Datei öffnen"/>
		<item id="3" type="textbox" value="Während des Downloads " default="Download"/>
		<item id="4" type="textbox" value="Download-Aktion" default="Jetzt herunterladen"/>
		<item id="6" type="textbox" value="Beschreibung der Web-Ressource"/>
		<item id="7" type="colorpicker" value="Farbe für gerade Zeilen" default="#FFFFFF"/>
		<item id="8" type="colorpicker" value="Farbe für ungerade Zeilen" default="#E9EDEF"/>
	</text>
	<text language="ru">
		<item id="1" type="textbox" value="Название коллекции"/>
		<item id="2" type="textbox" value="При открытии файла" default="Открыть файл"/>
		<item id="3" type="textbox" value="При загрузке файла" default="Загружается"/>
		<item id="4" type="textbox" value="Действие для загрузки" default="Загрузить сейчас"/>
		<item id="6" type="textbox" value="Описание Web Resource"/>
		<item id="7" type="colorpicker" value="Цвет для четных строк " default="#FFFFFF"/>
		<item id="8" type="colorpicker" value="Цвет для нечетных строк" default="#E9EDEF"/>
	</text>
	<text language="lt">
		<item id="1" type="textbox" value="Rinkinio pavadinimas"/>
		<item id="2" type="textbox" value="Atveriant failą" default="Atverti failą"/>
		<item id="3" type="textbox" value="Failui siunčiantis" default="Siunčiama"/>
		<item id="4" type="textbox" value="Atsisiuntimo veiksmas" default="Atsisiųsti dabar"/>
		<item id="6" type="textbox" value="Web resurso aprašymas"/>
		<item id="7" type="colorpicker" value="Lyginių eilučių spalva" default="#FFFFFF"/>
		<item id="8" type="colorpicker" value="Nelyginių eilučių spalva" default="#E9EDEF"/>
	</text>
	<text language="ja">
		<item id="1" type="textbox" value="コレクションタイトル"/>
		<item id="2" type="textbox" value="”開く”の表示" default="開く"/>
		<item id="3" type="textbox" value="ダウンロード中の表示" default="ダウンロード中"/>
		<item id="4" type="textbox" value="”ダウンロード”の表示" default="今すぐダウンロード"/>
		<item id="6" type="textbox" value="Webリソースの説明"/>
		<item id="7" type="colorpicker" value="遇数行の色" default="#FFFFFF"/>
		<item id="8" type="colorpicker" value="奇数行の色" default="#E9EDEF"/>
	</text>
	<!-- Better to place english translation at the end to exit data collection immediately when language specific data already loaded -->
	<text language="en">
		<item id="1" type="textbox" value="Collection title"/>
		<item id="2" type="textbox" value="File opening action" default="Open File"/>
		<item id="3" type="textbox" value="While downloading" default="Downloading"/>
		<item id="4" type="textbox" value="Download action" default="Download Now"/>
		<item id="6" type="textbox" value="Description of Web Resource"/>
		<item id="7" type="colorpicker" value="Color for even rows" default="#FFFFFF"/>
		<item id="8" type="colorpicker" value="Color for odd rows" default="#E9EDEF"/>
	</text>
</wizardText>
</template>