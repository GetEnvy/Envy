<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://schemas.getenvy.com/Template.xsd" version="1.0">
<!-- Originally created by Rolandas Rudomanskis -->
<manifest	name="Template for Video"
			author="Envy"
			version="1.0"
			link="http://getenvy.com"
			description="A template for video collections. With metadata for each file."
			icon="Video.ico"
/>
<files>
	<file type="main" path="VideoCollection.tpl"/>
	<file type="evenfile" path="EvenFile.tpl"/>
	<file type="oddfile" path="OddFile.tpl"/>
	<!-- <file path="other.tpl"/> -->
</files>
<images>
	<image path="Images\bar_on.gif"/>
	<image path="Images\bar_off.gif"/>
	<image path="Images\ScreenshotArea.png"/>
	<image path="Images\TopRepeat.png"/>
	<image path="Images\Top.png"/>
	<image path="Images\res.gif"/>
	<image path="Images\bg.png"/>
</images>
